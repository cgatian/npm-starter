{
  "name": "angular-webpack-npm-seed",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "angular"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "prebuild": "npm run clean",
    "build": "webpack",
    "postbuild": "npm run copy",
    "minify": "./node_modules/uglify-js/bin/uglifyjs dist/bundles/npm-module-seed.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/npm-module-seed.umd.min.js",
    "copy": "cpx \"./package-dist.json\" dist && renamer --find \"package-dist.json\" --replace \"package.json\" ./dist/*",
    "builds": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy"
  },
  "license": "MIT",
  "homepage": "",
  "bugs": {
    "url": ""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "cpx": "^1.5.0",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "typescript": "2.0.10",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@types/node": "^7.0.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "css-loader": "^0.26.1",
    "ngc-webpack": "^1.2.0",
    "raw-loader": "^0.5.1",
    "rxjs": "^5.0.3",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "webpack": "^2.2.1",
    "zone.js": "^0.7.6"
  }
}
